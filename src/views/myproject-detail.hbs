<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="/assets/css/bootstrap.min.css" rel="stylesheet">
    <link rel=" stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css"
        integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA=="
        crossorigin="anonymous" referrerpolicy="no-referrer">
    <link rel="stylesheet" href="/assets/css/myproject.css">
</head>

<body>
    <!-- Navbar -->
    <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid gap-3">
            <a class="navbar-brand ms-4" href="#">
                <img src="/assets/image/dw.png" style="height: 40px;">
            </a>
            <!-- toggler -->
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0 gap-3">
                    <li class="nav-item">
                        <a class="nav-link " href="/">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" href="/addmyproject">Add Project</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/testimonial">Testimonial</a>
                    </li>
                </ul>
                <a href="/contact">
                    <button class="btn btn-primary me-4" id="btn-nav" type="submit">Contact Me</button>
                </a>
            </div>
        </div>
    </nav>

    <!-- Isi Konten -->
    <div class="container mt-5">
        <h1 class="text-center mb-5">{{dataDetail.id}} {{dataDetail.project}}</h1>
        <div class="row justify-content-md-center">
            <div class="col-md-auto">
                <img src="/assets/image/logo3.jpg" class="img-fluid" alt="...">

            </div>
            <div class="col-md-auto ">
                <div class="row mb-3">
                    <h2>Duration</h2>
                    <div class="row">
                        <div class="col mb-3">
                            <div class="d-flex align-items-center gap-2">
                                <i class="fa-solid fa-calendar-days fa-sm"> </i>
                                <label>12 jan 2021 - 11 Feb 2021 </label>
                            </div>
                            <div class="d-flex align-items-center gap-2">
                                <i class="fa-solid fa-clock fa-sm"> </i>1 month
                            </div>
                        </div>
                    </div>

                </div>
                <div class="row mb-3">
                    <h2>Technologies</h2>
                    <div class="row">
                        <div class="col-md-4 mb-3">
                            <div class="d-flex align-items-center gap-2">
                                <i class="fa-brands fa-react fa-lg"></i>
                                <label>React js</label>
                            </div>
                            <div class="d-flex align-items-center gap-2">
                                <i class="fa-brands fa-js fa-lg"> </i>
                                <label>Java Script</label>
                            </div>
                        </div>
                        <div class="col-md-4 mb-3">
                            <div class="d-flex align-items-center gap-2">
                                <i class="fa-brands fa-node-js fa-lg "></i>
                                <label> Node Js</label>
                            </div>
                            <div class="d-flex align-items-center gap-2">
                                <i class="fa-brands fa-vuejs fa-lg"></i>
                                <label>vue js</label>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

            <div class="mt-5">
                <p align="justify">{{dataDetail.desc}}</p>
            </div>
        </div>
    </div>
    <script src="/assets/js/myproject.js"></script>
    <script src="/assets/js/bootstrap.bundle.min.js"></script>
</body>

</html>